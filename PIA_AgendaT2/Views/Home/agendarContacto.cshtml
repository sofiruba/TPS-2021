@{
    ViewData["Title"] = "Agendar contacto";
}


      <form method="post" action='@Url.Action("AgendarContacto","Home")'>
    Nombre: <input type="text" name="Nombre" class="form-control"/> </br> </br>
    Apellido: <input type="text" name="Apellido" class="form-control"/> </br> </br>
    Telefono: <input type="tel" name="Telefono" class="form-control" /> </br> </br>
    Email: <input type="email" name="Email" class="form-control" /> </br> </br>
    Foto(insertar link): <input type="text" name="URLFoto" class="form-control" /> </br> </br>
    <button type="submit" class="btn btn-primary">Enviar</button>

</form>
